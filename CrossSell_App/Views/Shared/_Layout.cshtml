</html>

<script type="text/javascript">
    $(function () {
        $('#urlddl').change(function () {
            var url = $(this).val();
            if (url != null && url != '') {
                window.location.href = url;
            }
        });
        $('#urlddl1').change(function () {
            var url = $(this).val();
            if (url != null && url != '') {
                window.location.href = url;
            }
        });
        $('#urlddl2').change(function () {
            var url = $(this).val();
            if (url != null && url != '') {
                window.location.href = url;
            }
		});
		$('#btn-search').on('click', function () {
			if ($('#search-text').val() != "" && $('#search-text').val() != null && $('#search-text').val() != undefined) {
				debugger;
				var NestId = $('#search-text').val();
				var url = "/Search/SearchAlgorithm?searchKey=" + NestId;
				window.location.href = url;
			}
			else {
				alert("Please enter text to be shared");
			}

		})
		function onSearchClick() {
			if ($('#search-text').val() != "" && $('#search-text').val() != null && $('#search-text').val() != undefined) {
				debugger;
				var NestId = $('#search-text').val();
				var url = "/Search/SearchAlgorithm?searchKey=" + NestId;
				window.location.href = url;
			}
			else {
				alert("Please enter text to be shared");
			}
		}
		$('#search-text').keypress(function (e) {
			debugger;
			var key = e.which;
			if (key == 13)  // the enter key code
			{
				onSearchClick();
				return false;
			}
		});

		$('#btn-search').keypress(function (e) {
			debugger;
			var key = e.which;
			if (key == 13)  // the enter key code
			{
				onSearchClick();
				return false;
			}
		});
	});

	

</script>