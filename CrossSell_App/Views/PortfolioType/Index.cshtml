@model IEnumerable<CrossSell_App.DataAccess.Portfolio_Type>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@*<h2>Index</h2>*@

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.Label("Portfolio Type")
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Portfolio_Type_Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Portfolio_Type_Id }) |
            @*@Html.ActionLink("Details", "Details", new { id=item.Portfolio_Type_Id }) |*@
            @Html.ActionLink("Delete", "Delete", new { id=item.Portfolio_Type_Id })
        </td>
    </tr>
}

</table>
